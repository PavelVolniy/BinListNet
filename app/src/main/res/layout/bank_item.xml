<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:orientation="vertical"
  android:layout_height="match_parent">

  <TextView
    android:id="@+id/tv_title_item_bank"
    style="@style/titleText"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="BANK" />

  <TextView
    android:id="@+id/tv_name_bank"
    style="@style/valueTextAction"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Jyske Bank, Hjorring" />

  <TextView
    android:id="@+id/tv_link_bank"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="www.jyskebank.dk"
    android:textColor="@color/link_color" />

  <TextView
    android:id="@+id/tv_phone_bank"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="+4589893300" />
</LinearLayout>